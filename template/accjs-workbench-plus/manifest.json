{
	"manifest_version": 2,
	"default_locale": "zh_CN",
	"name": "__MSG_pluginName__",
	"version": "3.0",
	"description": "__MSG_pluginDesc__",
	"author": "__MSG_pluginAuthor__",
	"homepage_url": "http://www.qt06.com",
	"icons":
	{
		"16": "icon/icon.png",
		"48": "icon/icon.png",
		"128": "icon/icon.png"
	},
	"browser_action": 
	{
		"default_icon": "icon/icon.png",
		"default_title": "__MSG_pluginName__",
		"default_popup": "html/index.html"
	},
	"content_scripts": 
	[
		{
			"matches": ["https://x-space.taobao.com/*"],
			"js": ["js/mousetrap.min.js", "js/workbench.min.js"],
			"run_at": "document_end"
		},
		{
			"matches": ["https://h5.m.taobao.com/xspace-component-mobile-workshift/*"],
			"js": ["js/mousetrap.min.js", "js/workshift-mobile.min.js"],
			"run_at": "document_end"
		},
		{
			"matches": ["https://alicert.taobao.com/cert-cloud/*"],
			"js": ["js/mousetrap.min.js", "js/learning.min.js"],
			"run_at": "document_end"
		},
		{
			"matches": ["https://alicert.taobao.com/exam/*"],
			"js": ["js/exam.min.js"],
			"run_at": "document_end"
		},
		{
			"matches": ["https://survey.aliyun.com/apps/zhiliao/*"],
			"js": ["js/survey.min.js"],
			"run_at": "document_end"
		},
		{
			"matches": ["https://survey.taobao.com/apps/zhiliao/*"],
			"js": ["js/survey.min.js"],
			"run_at": "document_end"
		}
	],
	"web_accessible_resources": 
	[
		"snd/*",
		"js/*"
	],
	"permissions":
	[
		"storage"
	],
	"options_ui":
	{
		"page": "html/index.html",
		// 添加一些默认的样式，推荐使用
		"chrome_style": true
	}

}